# Breez SDK API Key
# Your Primal Breez API key for authentication
PRIMAL_BREEZ_API_KEY=your_api_key_here

# Breez SDK Network
# Network to use: 'mainnet' for production or 'testnet' for testing
PRIMAL_BREEZ_NETWORK=mainnet

# Breez SDK Working Directory
# Local directory path where Breez SDK stores its data and state
PRIMAL_BREEZ_WORKING_DIR=./breez_data
